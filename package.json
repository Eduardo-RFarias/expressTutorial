{
  "name": "expresstutorial",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --watch 'src/' --exec 'yarn run lint:src && ts-node src/app.ts' -e ts",
    "start:deploy": "node build/app.js",
    "test": "yarn run lint:tests && jest",
    "build": "tsc && babel src --out-dir build --extensions \".ts,.tsx\"",
    "format": "prettier --write .",
    "lint:src": "eslint 'src/**'",
    "lint:tests": "eslint 'tests/**'"
  },
  "dependencies": {
    "express": "~4.17.2"
  },
  "devDependencies": {
    "@babel/cli": "~7.16.8",
    "@babel/core": "~7.16.12",
    "@babel/node": "~7.16.8",
    "@babel/preset-env": "~7.16.11",
    "@babel/preset-typescript": "~7.16.7",
    "@types/express": "~4.17.13",
    "@types/jest": "~27.4.0",
    "@types/node": "~17.0.13",
    "@types/supertest": "~2.0.11",
    "@typescript-eslint/eslint-plugin": "~5.10.1",
    "@typescript-eslint/parser": "~5.10.1",
    "eslint": "~8.8.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.25.4",
    "jest": "~27.4.7",
    "nodemon": "~2.0.15",
    "prettier": "~2.5.1",
    "supertest": "~6.2.2",
    "ts-jest": "~27.1.3",
    "ts-node": "~10.4.0",
    "typescript": "~4.5.5"
  }
}
